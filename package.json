{
  "name": "myorm_pg",
  "version": "10.0.1",
  "description": "A ORM writen in typescript for postgres",
  "main": "./lib/Index.js",
  "types": "./lib/Index.d.ts",
  "files": [
    "./lib",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./lib/Index.js",
      "require": "./lib/Index.js",
      "types": "./lib/Index.d.ts"
    },
    "./interfaces": {
      "import": "./lib/interfaces/Index.js",
      "require": "./lib/interfaces/Index.js",
      "types": "./lib/interfaces/Index.d.ts"
    },
    "./abstractions": {
      "import": "./lib/abstractions/Index.js",
      "require": "./lib/abstractions/Index.js",
      "types": "./lib/abstractions/Index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "interfaces": [
        "./lib/interfaces/Index.d.ts"
      ],
      "abstractions": [
        "./lib/abstractions/Index.d.ts"
      ]
    }
  },
  "readme": "README.md",
  "scripts": {
    "test": "npx jest --colors --runInBand"
  },
  "keywords": [
    "orm",
    "postgres",
    "database"
  ],
  "author": "adriano.marino1992@gmail.com",
  "repository": "https://github.com/adrianomarino1992/myorm_pg.git",
  "homepage": "https://github.com/adrianomarino1992/myorm_pg#readme",
  "license": "ISC",
  "dependencies": {
    "myorm_core": "^9.1.0",
    "pg": "^8.10.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/pg": "^8.6.6",
    "@types/reflect-metadata": "^0.1.0",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "ts-jest": "^29.1.0"
  }
}